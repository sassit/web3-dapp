{
	"name": "encode-web3",
	"packageManager": "yarn@3.5.1",
	"scripts": {
		"clean": "hh clean",
		"compile": "hh clean && hardhat compile",
		"test": "hh test tests/*.ts",
		"lint": "solhint 'contracts/*.sol'",
		"lint:fix": "solhint 'contracts/**/*.sol' --fix",
		"format": "prettier --write .",
		"coverage": "hh coverage --solcoverjs ./.solcover.js",
		"deploy:ballot": "hh --network mumbai run scripts/DeployTokenizedBallot.ts",
    "deploy:oracle": "hh --network mumbai run scripts/DeployOracle.ts",
    "run:vote": "hh --network mumbai run scripts/RunMyERC20Votes.ts"
	},
	"devDependencies": {
		"@chainlink/contracts": "^0.6.1",
		"@ethersproject/abi": "^5.4.7",
		"@ethersproject/providers": "^5.4.7",
		"@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
		"@nomicfoundation/hardhat-network-helpers": "^1.0.0",
		"@nomicfoundation/hardhat-toolbox": "^2.0.0",
		"@nomicfoundation/hardhat-verify": "^1.0.0",
		"@nomiclabs/hardhat-ethers": "^2.0.0",
		"@nomiclabs/hardhat-etherscan": "^3.1.7",
		"@openzeppelin/contracts": "^4.8.3",
		"@typechain/ethers-v5": "^10.1.0",
		"@typechain/hardhat": "^6.1.2",
		"@types/chai": "^4.2.0",
		"@types/lodash": "^4.14.194",
		"@types/mocha": ">=9.1.0",
		"@types/node": ">=12.0.0",
		"chai": "^4.2.0",
		"dotenv": "^16.0.3",
		"ethers": "^5.4.7",
		"hardhat": "^2.14.0",
		"hardhat-contract-sizer": "^2.8.0",
		"hardhat-gas-reporter": "^1.0.8",
		"hardhat-nodemon": "^1.1.0",
		"hardhat-shorthand": "^1.0.0",
		"lodash": "^4.17.21",
		"prettier": "^2.8.8",
		"prettier-plugin-solidity": "^1.1.3",
		"solhint": "^3.4.1",
		"solhint-plugin-prettier": "^0.0.5",
		"solidity-coverage": "^0.8.0",
		"ts-node": ">=8.0.0",
		"typechain": "^8.1.0",
		"typescript": ">=4.5.0",
		"usingtellor": "^5.0.4"
	},
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT"
}
